{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "isolatedModules": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "lib": [
            "esnext",
            "dom"
        ],
        "jsx": "react",
        "jsxFactory": "h",
        "allowJs": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}